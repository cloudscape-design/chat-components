{
  "compilerOptions": {
    "lib": ["es2019", "dom", "dom.iterable"],
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "rootDir": "src-themeable",
    "declaration": true,
    "incremental": true,
    "outDir": "lib/components-themeable",
    "skipLibCheck": true,
    "strict": true,
    "tsBuildInfoFile": "./.cache/themeable-source.tsbuildinfo"
